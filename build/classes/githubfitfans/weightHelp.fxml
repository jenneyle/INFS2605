<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="githubfitfans.WeightHelpController">
    <stylesheets>
        <URL value="@StyleSheet.css" />
    </stylesheets>
   <styleClass>
      <String fx:value="mainFxmlClass" />
      <String fx:value="leftBackground" />
   </styleClass>
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="46.0" layoutY="49.0" prefHeight="40.0" prefWidth="318.0" text="Your dashboard shows your weight goal" />
      <Label alignment="CENTER" layoutX="75.0" layoutY="14.0" prefHeight="35.0" prefWidth="265.0" styleClass="page-title" stylesheets="@StyleSheet.css" text="Colour Ratings " textAlignment="CENTER" />
      <HBox layoutX="83.0" layoutY="96.0" prefHeight="27.0" prefWidth="265.0">
         <children>
            <Circle fill="#aef49f" radius="13.0" stroke="BLACK" strokeType="INSIDE" />
            <Label text=" 0-2 kg away from your goal" />
         </children>
      </HBox>
      <HBox layoutX="83.0" layoutY="130.0" prefHeight="27.0" prefWidth="292.0">
         <children>
            <Circle fill="#fbeb72" radius="13.0" stroke="BLACK" strokeType="INSIDE" />
            <Label prefHeight="17.0" prefWidth="221.0" text=" 2-5 kg away from your goal" />
         </children>
      </HBox>
      <HBox layoutX="83.0" layoutY="163.0" prefHeight="27.0" prefWidth="257.0">
         <children>
            <Circle fill="#f8b286" radius="13.0" stroke="BLACK" strokeType="INSIDE" />
            <Label prefHeight="17.0" prefWidth="194.0" text=" 5-10 kg away from your goal" />
         </children>
      </HBox>
      <HBox layoutX="84.0" layoutY="197.0" prefHeight="27.0" prefWidth="242.0">
         <children>
            <Circle fill="#ff9e99" radius="13.0" stroke="BLACK" strokeType="INSIDE" />
            <Label prefHeight="17.0" prefWidth="209.0" text=" &gt;10 kg away from your goal" />
         </children>
      </HBox>
      <Button fx:id="back" layoutX="340.0" layoutY="259.0" mnemonicParsing="false" onAction="#handleBackButton" stylesheets="@StyleSheet.css" text="Back" textFill="#268265" />
      <HBox layoutX="84.0" layoutY="232.0" prefHeight="27.0" prefWidth="242.0">
         <children>
            <Circle fill="#b8e0d2" radius="13.0" stroke="BLACK" strokeType="INSIDE" />
            <Label prefHeight="17.0" prefWidth="209.0" text=" Enter weight goal in settings" />
         </children>
      </HBox>
   </children>
</AnchorPane>
